package oopIntro;

public class Product {
	int id;	
	String name;
	double unitPrice;
	String detail;
	double discount;//indirim
	
	public Product(int id, String name, double unitPrice, String detail, double discount) {
		super();
		this.id = id;
		this.name = name;
		this.unitPrice = unitPrice;
		this.detail = detail;
	}
	
	

}
