	}
	
	public void createdCourse(Instructor instructor) {
		System.out.println("Eðitmen: "+instructor.getFirstName()+" "+instructor.getLastName()+" "+instructor.getCreatedCourses()+" oluþturdu.");
	}
	
	public void createdCourse(Instructor instructor) {
		System.out.println("Eðitmen: "+instructor.getFirstName()+" "+instructor.getLastName()+" "+instructor.getCreatedCourses()+" oluþturdu.");
	}
