package oopIntro;

public class Product {
	
	public Product() {//CONSTRUCTOR METOD- Class ile ayný isimde public metod constructor - yapýcý metottur
		System.out.println("Ben çalýþtým");
	}
	
	public Product(int id, String name, double unitPrice, String detail) {
		this.id=id;
		this.name=name;
		this.unitPrice=unitPrice;
		this.detail=detail;
	}
	
	int id;
	String name;
	double unitPrice;
	String detail;
	
}
